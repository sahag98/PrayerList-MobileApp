{
  "expo": {
    "name": "Prayse",
    "slug": "prayer-app",
    "version": "7.1.2",
    "orientation": "portrait",
    "owner": "sahag98",
    "icon": "./assets/prayer.png",
    "userInterfaceStyle": "automatic",
    "notification": {
      "icon": "./assets/noti-image.png",
      "color": "#349eeb"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/noti-image.png",
          "color": "#349eeb"
        }
      ]
    ],
    "splash": {
      "image": "./assets/prayer-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "userInterfaceStyle": "automatic",
      "supportsTablet": true,
      "bundleIdentifier": "com.sahag98.prayerListApp",
      "buildNumber": "25"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/prayer.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.sahag98.prayerListApp",
      "googleServicesFile": "./google-services.json",
      "versionCode": 27
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "baaf16f2-e4cf-467c-87f4-0abb7a76c040"
      }
    }
  }
}